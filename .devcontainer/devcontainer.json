{
  "name": "pinch_customer_app",
  "context": "..",
  "dockerFile": "../Dockerfile.dev",
  "remoteUser": "developer",
  "settings": {
    "terminal.integrated.shell.linux": null
  },
  "runArgs": [
    "--privileged"
  ],
  "extensions": [
    "dart-code.flutter"
  ],
  "workspaceMount": "source=${localWorkspaceFolder},target=/app,type=bind,consistency=delegated",
  "workspaceFolder": "/app"
}